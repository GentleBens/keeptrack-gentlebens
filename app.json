{
  "expo": {
    "name": "keeptrack-gentlebens",
    "slug": "keeptrack-gentlebens",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/image/adaptive-icon.png",
    "splash": {
      "image": "./src/assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/image/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/image/favicon.png"
    },
    "description": "First push for counter app Gentle Ben's AZ",
    "githubUrl": "https://github.com/stacyburris/keeptrack-gentlebens"
  }
}
